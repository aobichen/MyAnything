@model IEnumerable<Anything.Models.SysManage>

<p>
    
</p>


<div class="panel panel-green">
    <div class="panel-heading">
        <div class="panel-title">
            系統設定
            <button type="button" class="btn btn-dark">AAA</button>
        </div>
    </div>
    <div class="panel-body">
        <div class="col-md-12 col-right">
            <h4>
                @Html.ActionLink("新增", "Create")
            </h4>
        </div>
        <div class="col-md-12">
            <table class="table table-striped table-condensed table-bordered table-hover alert-success">
                <tr>
                    <th>
                        欄位名稱
                    </th>
                    @*<th>
                            @Html.DisplayNameFor(model => model.FieldDescription)
                        </th>*@
                    <th>
                        值
                    </th>
                    <th>
                        單位
                    </th>
                    @*<th>
                            @Html.DisplayNameFor(model => model.Enabled)
                        </th>*@
                    @*<th>
                            @Html.DisplayNameFor(model => model.Editor)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Edited)
                        </th>*@
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.FieldName)
                        </td>
                        @*<td>
                                @Html.DisplayFor(modelItem => item.FieldDescription)
                            </td>*@
                        <td>
                            @Html.DisplayFor(modelItem => item.Value)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Unit)
                        </td>
                        @*<td>
                                @Html.DisplayFor(modelItem => item.Enabled)
                            </td>*@
                        @*<td>
                                @Html.DisplayFor(modelItem => item.Editor)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Edited)
                            </td>*@
                        <td>
                            @Html.ActionLink("編輯", "Edit", new { id = item.ID })
                            @*@Html.ActionLink("Details", "Details", new { id=item.ID }) |
                                @Html.ActionLink("Delete", "Delete", new { id=item.ID })*@
                        </td>
                    </tr>
                }

            </table>
        </div>
    </div>
</div>


<div>
   
</div>
